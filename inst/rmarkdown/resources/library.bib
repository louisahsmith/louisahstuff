@article{McDonnold2014,
  ids = {McDonnold2014a},
  title = {High risk human papillomavirus at entry to prenatal care and risk of preeclampsia},
  author = {McDonnold, Mollie and Dunn, Holly and Hester, Ashley and Pacheco, Luis D. and Hankins, Gary D.V. and Saade, George R. and Costantine, Maged M.},
