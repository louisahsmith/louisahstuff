@article{Werler2009,
  title = {Is there epidemiologic evidence to support vascular disruption as a pathogenesis of gastroschisis?},
  author = {Werler, Martha M. and Mitchell, Allen A. and Moore, Cynthia A. and Honein, Margaret A.},
  year = {2009},
