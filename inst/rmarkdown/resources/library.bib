  shorttitle = {Genetic and epigenetic regulation of {{AHR}} gene expr},
  author = {a. Englert, Neal and Turesky, Robert J. and Han, Weiguo and Bessette, Erin E. and Spivack, Simon D. and Caggana, Michele and Spink, David C. and Spink, Barbara C.},
  year = {2012},
