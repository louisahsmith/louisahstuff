@article{Schneeweiss1997,
  title = {Case\textendash crossover and case\textendash time\textendash control designs as alternatives in pharmacoepidemiologic research},
  author = {Schneeweiss, Sebastian and St{\"u}rmer, Til and Maclure, Malcolm},
  year = {1997},
  volume = {6},
  pages = {S51-S59},
  issn = {1099-1557},
  doi = {10.1002/(SICI)1099-1557(199710)6:3+<S51::AID-PDS301>3.0.CO;2-S},
  abstract = {Standard cohort and case\textendash control designs are suited to the study of cumulative effects of chronic exposures, but they are prone to confounding by indication. Case\textendash crossover and case\textendash time\textendash control studies are especially useful for studying intermittent exposures with transient effects, and are less susceptible to confounding by indication. Each design has its strengths and weaknesses. Despite the increasing availability of automated databases, cohort studies are usually time consuming and expensive, and therefore not preferred for time-critical decisions. In case\textendash control studies, the selection of appropriate controls can be difficult and time consuming, and sometimes impractical when the exposure is rare. Case\textendash crossover studies use the exposure history of each case as his or her own control to examine the effect of transient exposures on acute events. It further allows to study the time relationship of immediate effects to the exposure. This design eliminates between-person confounding by constant characteristics, including chronic indications. Because exposure data for the case and control periods are provided by the same person, the problems of differential recall may be reduced in many but not all case\textendash crossover studies. Bias can result from temporal changes in prescribing or within-person confounding, including transient indication or changes in disease severity. The case\textendash time\textendash control design is an elaboration of the case\textendash crossover design, which uses exposure history data from a traditional control group to estimate and adjust for the bias from temporal changes in prescribing. This paper will present a structured decision table of when to use which design in pharmacoepidemiologic research. In conclusion, case\textendash crossover and case\textendash time\textendash control studies are the designs of choice when separating acute effects from chronic effects of transient exposures and if confounding by indication is an outstanding problem. \textcopyright{} 1997 John Wiley \& Sons, Ltd.},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/\%28SICI\%291099-1557\%28199710\%296\%3A3\%2B\%3CS51\%3A\%3AAID-PDS301\%3E3.0.CO\%3B2-S},
  file = {/Users/louisahsmith/Zotero/storage/QWNILW3W/(SICI)1099-1557(199710)63+S51AID-PDS3013.0.html},
  journal = {Pharmacoepidemiology and Drug Safety},
  keywords = {case-crossover,case-time-control,epidemiologic methods,pharmacoepidemiology,study design},
  language = {en},
  number = {S3}
}

@article{Schnitzer2020,
