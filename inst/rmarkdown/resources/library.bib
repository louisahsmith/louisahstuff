  journal = {Clinical Trials},
  keywords = {antiretroviral therapy,causality,clinical trial,external validity,generalizability,HIV/AIDS,intention-to-treat effect,inverse probability weighting,per-protocol effect,virologic failure},
  number = {1}
}

@article{Lubeck1996,
