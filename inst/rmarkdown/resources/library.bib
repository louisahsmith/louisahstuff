@article{OFarrell2015,
  ids = {OFarrell2015a},
  title = {Risk and timing of cardiovascular disease after androgen-deprivation therapy in men with prostate cancer},
  author = {O'Farrell, Sean and Garmo, Hans and Holmberg, Lars and Adolfsson, Jan and Stattin, P{\"a}r and Van Hemelrijck, Mieke},
  year = {2015},
