  author = {Svensson, Elisabeth and Ehrenstein, Vera and N{\o}rgaard, Mette and Bakketeig, Leiv Sigmund and Rothman, Kenneth J. and S{\o}rensen, Henrik Toft and Pedersen, Lars},
