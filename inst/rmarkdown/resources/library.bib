@article{Canan2017,
  ids = {Canan2017a},
  title = {Instrumental variable analyses and selection bias},
  author = {Canan, Chelsea and Lesko, Catherine and Lau, Bryan and Lauc, Bryan},
  year = {2017},
  volume = {28},
  pages = {396--398},
  issn = {15315487},
  doi = {10.1097/EDE.0000000000000639},
  abstract = {Instrumental variables (IV) are used to draw causal conclusions about the effect of exposure E on outcome Y in the presence of unmeasured confounders. IV assumptions have been well described: (1) IV affects E; (2) IV affects Y only through E; (3) IV shares no common cause with Y. Even when these assumptions are met, biased effect estimates can result if selection bias allows a noncausal path from E to Y. We demonstrate the presence of bias in IV analyses on a sample from a simulated dataset, where selection into the sample was a collider on a noncausal path from E to Y. By applying inverse probability of selection weights, we were able to eliminate the selection bias. IV approaches may protect against unmeasured confounding but are not immune from selection bias. Inverse probability of selection weights used with IV approaches can minimize bias.},
  file = {/Users/louisahsmith/Zotero/storage/FTN3VMBU/Canan et al._2017_Instrumental variable analyses and selection bias.pdf;/Users/louisahsmith/Zotero/storage/W93AF6L7/Canan, Lesko, Lauc_2017_Instrumental variable analyses and selection bias(2).pdf},
  isbn = {0000000000000},
  journal = {Epidemiology},
  number = {3},
  pmid = {28169934}
}

