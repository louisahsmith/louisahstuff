  file = {/Users/louisahsmith/Zotero/storage/SDK3ZF5W/Schnitzer et al. - 2020 - A tutorial on dealing with time-varying eligibilit.pdf;/Users/louisahsmith/Zotero/storage/P6F9YKBP/sim.html},
