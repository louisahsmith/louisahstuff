@article{Neugebauer2014a,
  ids = {Neugebauer2014,Neugebauer2014b},
  title = {Targeted learning in real-world comparative effectiveness research with time-varying interventions},
  author = {Neugebauer, Romain and Schmittdiel, Julie A. and {van der Laan}, Mark J.},
  year = {2014},
