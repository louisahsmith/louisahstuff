  keywords = {ADHD,Hyperactivity-impulsivity,Inattention,Maternal lifestyle during pregnancy,Prenatal exposure,Timing},
  language = {en},
  number = {3},
  pmid = {15755301}
}

@article{Rodriguez2008,
