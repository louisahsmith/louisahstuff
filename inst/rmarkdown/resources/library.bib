@article{Spangmose2017,
  title = {Academic performance in adolescents born after {{ART}} - {{A}} nationwide registry-based cohort study},
  author = {Spangmose, A. L. and Malchau, S. S. and Schmidt, L. and Vassard, D. and Rasmussen, S. and Loft, A. and Forman, J. and Pinborg, A.},
  year = {2017},
  volume = {32},
