  isbn = {0730-7268},
  journal = {Environmental Toxicology and Chemistry},
  keywords = {2;3;7;8-tetrachlorodibenzo-p-dioxin exposure,adaptation,ah,aromatic hydrocarbons,aryl hydrocarbon receptor pathway,atlantic tomcod,cytochrome P4501A,elizabeth river,fish,Fundulus heteroclitus,in-vivo,messenger-rna induction,nonimpacted,polycyclic,rainbow-trout,receptor,subpopulations,tissue-specific expression},
