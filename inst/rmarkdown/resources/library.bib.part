keywords = {Animals,Antimalarials/therapeutic use,Bedding and Linens,Chemoprevention,Female,Humans,Infant- Newborn,Insecticides/pharmacology,Malaria- Falciparum/drug therapy/parasitology/*pre,Mosquito Control/methods,Plasmodium falciparum/drug effects,Pregnancy,Pregnancy Complications- Parasitic/drug therapy/pa},
language = {eng},
mendeley-tags = {Animals,Antimalarials/therapeutic use,Bedding and Linens,Chemoprevention,Female,Humans,Infant- Newborn,Insecticides/pharmacology,Malaria- Falciparum/drug therapy/parasitology/*pre,Mosquito Control/methods,Plasmodium falciparum/drug effects,Pregnancy,Pregnancy Complications- Parasitic/drug therapy/pa},
month = {feb},
