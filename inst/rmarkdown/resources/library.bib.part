@article{May2010,
abstract = {Endometriosis is estimated to affect 1 in 10 women during the reproductive years. There is often delay in making the diagnosis, mainly due to the non-specific nature of the associated symptoms and the need to verify the disease surgically. A biomarker that is simple to measure could help clinicians to diagnose (or at least exclude) endometriosis; it might also allow the effects of treatment to be monitored. If effective, such a marker or panel of markers could prevent unnecessary diagnostic procedures and/or recognize treatment failure at an early stage.},
author = {May, K. E. and Conduit-Hulbert, S. a. and Villar, J. and Kirtley, S. and Kennedy, S. H. and Becker, C. M.},
