keywords = {Alcohol Drinking,Alcohol Drinking: adverse effects,Alcohol Drinking: prevention {\&} control,Child,Epidemiologic Research Design,Female,Fetus,Fetus: drug effects,Guidelines as Topic,Health Policy,Humans,Meta-Analysis as Topic,Pregnancy,Pregnancy Complications,Pregnancy Complications: prevention {\&} control,Prenatal Exposure Delayed Effects,Prenatal Exposure Delayed Effects: prevention {\&} co,Risk Factors},
language = {en},
