}
@article{Weiss2014a,
abstract = {Emotion regulation in ASD. Recent research suggest that emotion regulation may play an important role in the psychopathology/comorbidity spin problems persons with ASD experience. There are multiple ways used to assess emotion regulation. $\backslash$n$\backslash$nEmotion regulation are conscious and unconscious strategies implemented in response to emotion-eliciting stimuli. Gross and thompson have model of emotion regulation, suggesting 5 domains:$\backslash$n- Situation selection$\backslash$n- Situation modification $\backslash$n- attentional deployment$\backslash$n- cognitive change: modification of emotional reactions through appraisals of a situation and the ability to cope with it$\backslash$n- Response modulation: continuum of behavioral and physiological ways of regulating and expressing emotions after they are experiences.$\backslash$n$\backslash$nReview looks at different authors looked at emotion regulation in persons with ASD$\backslash$n$\backslash$n4 studies used physiological methods to study emotion regulation$\backslash$n$\backslash$nReview highlights the importance of using multiple methods of assessment to get a clear view of emotion regulation. },
