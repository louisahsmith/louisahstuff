journal = {Acta Obstetricia et Gynecologica Scandinavica},
keywords = {2002,AfricanâAmerican,BMI,IQ,accepted 20 october,african,american,bmi,conducted relat-,considerable research has been,iq,prepregnancy weight,submitted 8 october},
