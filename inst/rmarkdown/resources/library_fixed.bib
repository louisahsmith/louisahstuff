journal = {Journal of Clinical Pharmacology},
keywords = {Bias,folic acid antagonists,terminations of pregnancy},
number = {1},
pages = {78--83},
pmid = {21343345},
title = {Bias toward the null hypothesis in pregnancy drug studies that do not include data on medical terminations of pregnancy: The folic acid antagonists},
volume = {52},
year = {2012}
}
