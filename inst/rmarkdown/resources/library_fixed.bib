journal = {British Journal of Nutrition},
keywords = {*Birth Weight,Adipose Tissue/physiology,Adult,Body Composition/*physiology,Female,Gestational Age,Gravidity/physiology,HIV Infections/*epidemiology,Humans,Infant,Infectious/*epidemiology,Low Birth Weight/physiology,Male,Maternal Age,Multiple/physiology,Newborn,Pregnancy,Pregnancy Complications,Pregnancy Outcome/epidemiology,Prospective Studies,Risk Factors,Seasons,Time Factors,Viral Load,Zimbabwe/epidemiology},
language = {en},
