author = {Cauchemez, Simon and Besnard, Marianne and Bompard, Priscillia and Dub, Timoth{\'{e}}e and Guillemette-Artur, Prisca and Eyrolle-Guignot, Dominique and Salje, Henrik and {Van Kerkhove}, Maria D. and Abadie, V{\'{e}}ronique and Garel, Catherine and Fontanet, Arnaud and Mallet, Henri-Pierre},
doi = {10.1016/S0140-6736(16)00651-6},
