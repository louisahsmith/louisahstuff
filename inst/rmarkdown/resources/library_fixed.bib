keywords = {Depression,Health practices,Pregnancy outcomes,Psychosocial profile},
month = jan,
number = {3},
pages = {277--285},
pmid = {16553174},
title = {The relationship between psychosocial profile, health practices, and pregnancy outcomes},
volume = {85},
year = {2006}
}
