@article{Kristensen2015,
abstract = {Studies have examined the association between birth weight and intelligence, with the understanding that these are both related to a family's socioeconomic status. In sibling studies, the association between family socioeconomic position and intelligence is strong. Several studies have reported a lower mean intelligence score for those with the highest birth weights. The present sibling design study was performed to examine the apparent lower intellectual performance of those with birth weights in the highest range. Confounders were birth order, family socioeconomic position, or shared but unidentified family factors., The study population included all Norwegian male singletons (N = 242,771) born at term between 1967 and 1976 and later conscripted into the military. After exclusions, the study population included 217,746 men. Siblings were identified using the mother's identity. Two subset populations were categorized; the sibling subset included 137,574 men (of 104,893 mothers) with at least 1 sibling, and the fraternal population included 62,906 men who had at least 1 brother born at term. Participants were examined between 1984 and 2003, when 98.3{\%} were aged 18 to 19 years. The intellectual performance test included 3 timed components: arithmetic, word similarities, and figures. These test results are highly correlated with IQ scores based on the Wechsler Abbreviated Scales of Intelligence. To aid interpretation, the stanine test scores were transformed to an IQ scale with a mean of 100 and an SD of 15. Birth weight was classified into 8 levels with less than 2000 g the lowest and 5000 g or greater the highest. Birth weights of 4500 g or greater were considered macrosomic. Covariates included birth, maternal, and paternal factors. Primary analyses examined the association between birth weight, particularly macrosomia, and intelligence using a birth weight of 4000 to 4499 g as the reference., The mean birth weight was 3595 +/- 490 g, and the mean IQ score was 101.7 +/- 13.7 in the total study population. Mean IQ scores increased steadily from the lowest birth weight, reaching a peak value at 4000 to 4499 g, and declining in the 2 highest weight categories. The crude [beta] coefficient for birth weights of 5000 g or greater was -1.2 (95{\%} confidence interval, -2.2 to -0.3) compared with the reference weight. In the total population, 7826 births (3.6{\%}) were macrosomic, and 6832 (4.9{\%}) of the 137,574 sibling subset had sibship macrosomia. Mean differences in IQ scores were much larger for parental education level and somewhat larger for birth order than for birth weight. Other covariates were more weakly associated with IQ score. Sibship macrosomia, parental education, and birth order confounded the exposure-outcome association, and macrosomic men with birth injury or asphyxia had low mean IQ scores. The differences seen leveled off for birth weights of 4500 g or greater in the multivariable analysis, because of the influence of birth order and parental education. Including participants with injury or asphyxia in the analysis did not substantially alter associations between birth weight and IQ score., The majority of the association between high birth weight and low IQ score was caused by confounding from shared family factors. Participants from families with a tendency to have macrosomic babies had, on average, lower IQ scores than did other participants; however, men with higher birth weights tended to have higher IQ scores. Future research should attempt to identify the shared family factors contributing to the association. The creation of large prospective birth cohorts with sibling designs and application of multilevel analysis is necessary., Copyright (C) 2015 Wolters Kluwer Health, Inc. All rights reserved.},
author = {Kristensen, Petter and Susser, Ezra and Irgens, Lorentz M. and Mehlum, Ingrid Sivesind and Corbett, Karina and Bjerkedal, Tor},
doi = {10.1093/aje/kwu241},
isbn = {0029-7828},
issn = {0002-9262},
journal = {American Journal of Epidemiology},
language = {en},
month = nov,
number = {9},
pages = {876--884},
shorttitle = {The Association of High Birth Weight With Intellig},
title = {The Association of High Birth Weight With Intelligence in Young Adulthood: A Cohort Study of Male Siblings},
volume = {180},
year = {2014}
}
@article{Vandenbroucke2015a,
abstract = {The idea that epidemiologic studies should start from first exposure onward has been advocated in the past few years. The study of incident exposures is contrasted with studies of prevalent exposures in which follow-up may commence after first exposure. The former approach is seen as a hallmark of a good study and necessary for causal inference. We argue that studying incident exposures may be necessary in some situations, but it is not always necessary and is not the preferred option in many instances. Conducting a study involves decisions as to which person-time experience should be included. Although studies of prevalent exposures involve left truncation (missingness on the left), studies of incident exposures may involve right censoring (missingness on the right) and therefore may not be able to assess the long-term effects of exposure. These considerations have consequences for studies of dynamic (open) populations that involve a mixture of prevalent and incident exposures. We argue that studies with prevalent exposures will remain a necessity for epidemiology. The purpose of this paper is to restore the balance between the emphasis on first exposure cohorts and the richness of epidemiologic information obtained when studying prevalent exposures.},
author = {Vandenbroucke, Jan and Pearce, Neil},
doi = {10.1093/aje/kwv225},
issn = {14766256},
journal = {American Journal of Epidemiology},
keywords = {dynamic populations,epidemiologic methods,incidence rate,incident exposure,left truncation,prevalent exposure,right censoring,study design},
number = {10},
pages = {826--833},
title = {Point: Incident Exposures, Prevalent Exposures, and Causal Inference: Does Limiting Studies to Persons Who Are Followed from First Exposure Onward Damage Epidemiology?},
volume = {182},
year = {2015}
}
@article{Cooperstock1986,
abstract = {Marked circannual variation in the incidence of preterm birth, adjusted for the normal seasonal fertility rate, was observed in data from the Collaborative Perinatal Project collected at 12 urban university centers during 1959â1966 (p{\textless}10â5). The variation in monthly preterm birth rates ranged from a trough of 64{\%} in May to a peak of 144{\%} in September. This is considerably greater seasonal variation than found in most previous studies, which used differing definitions of prematurity. The seasonal trend was present for maternal age groups 22â26 years (p{\textless}0.005) and greater than 26 years (p{\textless}0.005), for Bureau of the Census socioeconomic index 3.6â4.9 (p{\textless}10â4) and greater than 4.9 (p{\textless}0.01), for those living in northern states (p{\textless}10â4) for married gravidas (p{\textless}10â6) for blacks (p{\textless}0.05) and whites (p{\textless}0.001), but not for those less than age 22, those with socioeconomic index less than 3.6, those living in southern states, and those not married. Thus, preterm birth was seasonal in the population studied, and the seasonal factor was more evident in demographic groups less predisposed to deliver preterm. These findings may have implications for the pathogenesis of a portion of premature deliveries, and should be considered in the design of studies related to the pathogenesis of preterm birth.},
author = {Cooperstock, Michael and Wolfe, Robert A. and Coopserstock, Michael and Wolfe, Robert A.},
