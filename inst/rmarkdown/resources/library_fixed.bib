@article{Bateman2015,
abstract = {OBJECTIVE: To examine the teratogenic potential of statins.$\backslash$n$\backslash$nDESIGN: Cohort study.$\backslash$n$\backslash$nSETTING: United States.$\backslash$n$\backslash$nPARTICIPANTS: A cohort of 886 996 completed pregnancies linked to liveborn infants of women enrolled in Medicaid from 2000 to 2007.$\backslash$n$\backslash$nMETHODS: We examined the risk of major congenital malformations and organ specific malformations in offspring associated with maternal use of a statin in the first trimester. Propensity score based methods were used to control for potential confounders, including maternal demographic characteristics, obstetric and medical conditions, and use of other drugs.$\backslash$n$\backslash$nRESULTS: 1152 (0.13{\%}) women used a statin during the first trimester. In unadjusted analyses, the prevalence of malformations in the offspring of these women was 6.34{\%} compared with 3.55{\%} in those of women who did not use a statin in the first trimester (relative risk 1.79, 95{\%} confidence interval 1.43 to 2.23). Controlling for confounders, particularly pre-existing diabetes, accounted for this increase in risk (1.07, 0.85 to 1.37). There were also no statistically significant increases in any of the organ specific malformations assessed after accounting for confounders. Results were similar across a range of sensitivity analyses.$\backslash$n$\backslash$nCONCLUSIONS: Our analysis did not find a significant teratogenic effect from maternal use of statins in the first trimester. However, these findings need to be replicated in other large studies, and the long term effects of in utero exposure to statins needs to be assessed, before use of statins in pregnancy can be considered safe.},
author = {Bateman, Brian T and Hern{\'{a}}ndez-D{\'{i}}az, Sonia and Fischer, Michael A and Seely, Ellen W and Ecker, Jeffrey L and Franklin, Jessica M and Desai, Rishi J and Allen-Coleman, Cora and Mogun, Helen and Avorn, Jerry and Huybrechts, Krista F},
doi = {10.1136/bmj.h1035},
isbn = {1756-1833; 0959-535X},
issn = {1756-1833},
journal = {BMJ (Clinical research ed.)},
pages = {h1035},
pmid = {25784688},
title = {Statins and congenital malformations: cohort study.},
volume = {350},
year = {2015}
}
