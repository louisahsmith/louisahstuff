author = {Neugebauer, Romain and van der Laan, Mark J.},
doi = {10.1016/j.csda.2006.06.013},
issn = {01679473},
journal = {Computational Statistics {\&} Data Analysis},
keywords = {causal inference,g-computation,longitudinal data,marginal structural model,maximum likelihood,nonparametric},
language = {en},
month = dec,
number = {3},
pages = {1664--1675},
shorttitle = {Causal effects in longitudinal studies},
title = {Causal effects in longitudinal studies: Definition and maximum likelihood estimation},
