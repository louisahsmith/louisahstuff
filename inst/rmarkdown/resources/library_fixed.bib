@article{Hernan2008a,
abstract = {BACKGROUND:: The Women's Health Initiative randomized trial found greater coronary heart disease (CHD) risk in women assigned to estrogen/progestin therapy than in those assigned to placebo. Observational studies had previously suggested reduced CHD risk in hormone users. METHODS:: Using data from the observational NursesÄâ¬â¢ Health Study, we emulated the design and intention-to-treat (ITT) analysis of the randomized trial. The observational study was conceptualized as a sequence of Äâ¬ trials,Äâ¬ in which eligible women were classified as initiators or noninitiators of estrogen/progestin therapy. RESULTS:: The ITT hazard ratios (HRs) (95{\%} confidence intervals) of CHD for initiators versus noninitiators were 1.42 (0.92Äâ¬"2.20) for the first 2 years, and 0.96 (0.78Äâ¬"1.18) for the entire follow-up. The ITT HRs were 0.84 (0.61Äâ¬"1.14) in women within 10 years of menopause, and 1.12 (0.84Äâ¬"1.48) in the others (P value for interaction â€ 0.08). These ITT estimates are similar to those from the Women's Health Initiative. Because the ITT approach causes severe treatment misclassification, we also estimated adherence-adjusted effects by inverse probability weighting. The HRs were 1.61 (0.97Äâ¬"2.66) for the first 2 years, and 0.98 (0.66Äâ¬"1.49) for the entire follow-up. The HRs were 0.54 (0.19Äâ¬"1.51) in women within 10 years after menopause, and 1.20 (0.78Äâ¬"1.84) in others (P value for interaction â€ 0.01). We also present comparisons between these estimates and previously reported NursesÄâ¬â¢ Health Study estimates. CONCLUSIONS:: Our findings suggest that the discrepancies between the Women's Health Initiative and NursesÄâ¬â¢ Health Study ITT estimates could be largely explained by differences in the distribution of time since menopause and length of follow-up. {\textcopyright} 2008 by Lippincott Williams {\&} Wilkins.},
author = {Hern{\'{a}}n, Miguel A. and Alonso, Alvaro and Logan, Roger and Grodstein, Francine and Michels, Karin B. and Willett, Walter C. and Manson, Joann E. and Robins, James M.},
