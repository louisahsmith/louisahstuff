journal = {Neuropharmacology},
keywords = {Cytokine,Hippocampus,Learning,Memory,Morphogenesis,Neurotrophic factors},
language = {en},
month = may,
number = {6},
pages = {903--917},
pmid = {15829260},
title = {Specific neurodevelopmental damage in mice offspring following maternal inflammation during pregnancy},
volume = {48},
year = {2005}
}
