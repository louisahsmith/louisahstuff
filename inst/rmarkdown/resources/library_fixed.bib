@article{Hair2015,
abstract = {Poverty is tied to structural differences in several areas of the brain associated with school readiness skills, with the largest influence observed among children from the poorest households. Regional gray matter volumes of children below 1.5 times the federal poverty level were 3 to 4 percentage points below the developmental norm (P {\textless} .05). A larger gap of 8 to 10 percentage points was observed for children below the federal poverty level (P {\textless} .05). These developmental differences had consequences for children's academic achievement. On average, children from low-income households scored 4 to 7 points lower on standardized tests (P {\textless} .05). As much as 20{\%} of the gap in test scores could be explained by maturational lags in the frontal and temporal lobes.},
