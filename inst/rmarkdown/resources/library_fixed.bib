language = {en},
mendeley-tags = {Folder - Development Outcomes},
month = sep,
number = {11},
pages = {1159--1166},
pmid = {19724967},
title = {Relationships among birth-month distribution, skeletal age and anthropometric characteristics in adolescent elite soccer players.},
volume = {27},
year = {2009}
