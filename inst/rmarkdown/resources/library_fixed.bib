issn = {0300-5771},
journal = {International journal of epidemiology},
keywords = {Adult,British Columbia,British Columbia: epidemiology,Cohort Studies,European Continental Ancestry Group,Female,Humans,Indians,Infant,Infant Mortality,Logistic Models,Male,Maternal Welfare,Newborn,North American,Pregnancy,Pregnancy Outcome,Risk,Rural Population,Urban Population},
language = {en},
month = dec,
number = {6},
pages = {1252--9},
pmid = {15319396},
shorttitle = {Infant mortality among First Nations versus non-Fi},
title = {Infant mortality among First Nations versus non-First Nations in British Columbia: temporal trends in rural versus urban areas, 1981-2000.},
