}
@article{Heggeseth2013,
abstract = {Longitudinal studies play a prominent role in health, social and behavioral sciences as well as in the biological sciences, economics, and marketing. By following subjects over time, temporal changes in an outcome of interest can be directly observed and studied. An important question concerns the existence of distinct trajectory patterns. One way to determine these distinct patterns is through cluster analysis, which seeks to separate objects (subjects, patients, observational units) into homogeneous groups. Many methods have been adapted for longitudinal data, but almost all of them fail to explicitly group trajectories according to distinct pattern shapes. To fulï¬ll the need for clustering based explicitly on shape, we propose vertically shifting the data by subtracting the subject-speciï¬c mean directly removes the level prior to ï¬tting a mixture modeling. This non-invertible transformation can result in singular covariance matrixes, which makes mixture model estimation difï¬cult. Despite the challenges, this method outperforms existing clustering methods in a simulation study},
author = {Heggeseth, Brianna C and Jewell, Nicholas P},
title = {Vertically Shifted Mixture Models for Clustering Longitudinal Data by Shape Vertically Shifted Mixture Models for Clustering Longitudinal Data by Shape},
year = {2013}
}
