isbn = {1476-6256 (Electronic)$\backslash$r0002-9262 (Linking)},
issn = {00029262},
journal = {American Journal of Epidemiology},
keywords = {bias (epidemiology),simulation},
number = {10},
pages = {938--948},
pmid = {23100247},
publisher = {Oxford University Press},
title = {Implications of m bias in epidemiologic studies: A simulation study},
volume = {176},
year = {2012}
}
