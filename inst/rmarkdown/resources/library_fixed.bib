journal = {Journal of Adolescent Health},
keywords = {Adolescents,Body image,Body mass index,Externalizing problem behavior,Internalizing problem behavior,Problem behavior,Youth Self-Report},
language = {en},
month = jul,
number = {1},
pages = {27--34},
pmid = {16781958},
title = {Body Mass Index and Body Weight Perception as Risk Factors for Internalizing and Externalizing Problem Behavior Among Adolescents},
