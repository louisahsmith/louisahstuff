keywords = {United States,adult,article,birth certificate,body mass,child,cohort analysis,controlled study,female,human,infant,intellectual impairment,major clinical study,male,morbid obesity,obesity,outcome assessment,pregnancy,priority journal,retrospective study,risk assessment,risk factor,underweight,weight change,weight gain,weight reduction},
language = {en},
