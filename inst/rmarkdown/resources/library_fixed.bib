journal = {Paediatric and Perinatal Epidemiology},
number = {3},
pages = {181--193},
pmid = {15860077},
title = {Risk factors for spontaneous preterm birth among Aboriginal and non-Aboriginal women in Manitoba},
volume = {19},
year = {2005}
}
