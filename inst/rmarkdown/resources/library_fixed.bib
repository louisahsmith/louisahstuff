@article{Frenk2017,
abstract = {Most improvements in health achieved since the early 20th century can be reasonably attributed to the advance-ment and application of knowledge. The latter occurs through three main mechanisms: its transformation into health technologies, such as drugs, vaccines, and diagnostic procedures; its internalization by individuals, who use it to guide their everyday life in crucial domains for health, such as personal hygiene, nutrition, and sexual behavior, among others; and finally, its translation into evidence that pro-vides a scientific foundation both for health care and for policy formulation, implementation, and evaluation [1]. There are three basic reasons why knowledge should increasingly support policy design, implementation, and evaluation. First, we are living in a time of unparalleled and accelerated transformation. Low-and middle-income countries, in particular, are witnessing a complex epidemi-ological transition characterized by a triple burden of dis-ease: the unfinished agenda of common infections, undernutrition, and maternal mortality; the emerging chal-lenges of noncommunicable diseases (NCDs), mental prob-lems, and injury; and the health risks directly related to globalization, such as climate change and pandemics [2]. The use of evidence in the documentation and discussion of the determinants, nature, magnitude, and distribution of health challenges is critical. Second, the search for universal health coverage (UHC), which is becoming a core component of the post-2015 development agenda, demands major innovations. The still vast set of unanswered questions about health system per-formance demands the inclusion of a research agenda in every policy initiative. The third reason is accountability. Due to the increasing awareness about the contribution of health to global secu-rity, economic growth, and democratic governance, devel-opment assistance for health grew from US{\$}10 billion in 2000 to over US{\$}28 billion in 2012 [3]. However, to secure the sustainability of these unprecedented financial efforts, we need to guarantee, in the words of the late Professor Ramalingaswami of India, not only more money for health but also more health for the money. In sum, the world as a whole requires a new generation of innovations, not just in conventional technologies, but also in the way we organize and finance health systems. In this search, knowledge must be the driving force. Two social policy innovations implemented in Mexico illustrate the uses of evidence to improve nutrition status, health conditions, and financial protection in health. The first example is Oportunidades (or Opportunities in En-glish), a ''conditional cash transfer'' (CCT) program intro-duced in the late-1990s to enhance basic capabilities of families living in extreme poverty. This program offers in-come supplements to poor families willing to fulfill certain elements of coresponsibility. To receive the cash transfer, Oportunidades families were originally required to send children to school rather than work; attend a health clinic to receive a package of health promotion and disease pre-vention interventions, including vaccines; and provide a nutritional supplement to pregnant and lactating women, all children aged 6e23 months and low-weight children aged 2e5 years [4]. In a decision that proved to be far sighted, the team in charge of Oportunidades, headed by a visionary demogra-pher, the late Jos e G omez de Le on, decided to evaluate its impact using a controlled randomized design [5]. In Conflict of interest: I, Till Bâ¬ arnighausen, declare that I have no conflict of interest regarding this manuscript (Frenk, Octavio G omez-Dant es; ''Systematic Generation of Evidence through Public Policy Evaluation''), which I am submitting as one of the guest editors of the themed issue of the Journal of Clinical Epidemiology on ''Quasi-experimental study designs in health systems research and synthesis''.},
author = {Frenk, Julio and G{\'{o}}mez-Dant{\'{e}}s, Octavio},
doi = {10.1016/j.jclinepi.2017.03.013},
issn = {18785921},
journal = {Journal of Clinical Epidemiology},
pages = {17--20},
pmid = {28365310},
title = {Quasi-experimental study designs seriesâpaper 3: systematic generation of evidence through public policy evaluation},
volume = {89},
year = {2017}
}
