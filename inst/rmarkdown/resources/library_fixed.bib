journal = {Journal of Child and Family Studies},
keywords = {Child psychosocial functioning,Domestic violence,Interparental aggression},
number = {5},
pages = {907--916},
pmid = {25132790},
title = {The Relationship Between Male-Perpetrated Interparental Aggression, Paternal Characteristics, and Child Psychosocial Functioning},
volume = {23},
year = {2014}
}
