isbn = {1464-3685 (Electronic)$\backslash$r0300-5771 (Linking)},
issn = {1464-3685},
journal = {International journal of epidemiology},
keywords = {Adult,Child,Child Development,Child, Preschool,Cognition,Cognition: physiology,Female,Gestational Age,Guidelines as Topic,Humans,Infant,Infant, Newborn,Institute of Medicine (U.S.),Intelligence Tests,Pregnancy,Regression Analysis,United States,Weight Gain,Weight Gain: physiology},
language = {en},
month = apr,
number = {2},
pages = {414--22},
