@article{Ezugwu2015,
abstract = {The number of embryos transferred after in vitro fertilization (IVF)$\backslash$nhave been a topic of debate for over a decade now. Due to the risk$\backslash$nassociated with multiple pregnancy, there has been a global effort at$\backslash$nreducing the multiple pregnancy rates to a minimum while maintaining an$\backslash$nacceptable level of successful IVF pregnancy rate. Elective single$\backslash$nembryo transfer (eSET) is advocated in most European countries. In$\backslash$nBelgium and Sweden, eSET is mandatory for couples with a good prognosis.$\backslash$nHowever, despite clinical recommendations and policy statements,$\backslash$npatients in clinical practice frequently do request for the transfer of$\backslash$nmultiple embryos in order to have twins. Such requests conflict with$\backslash$npolicy guidelines and create an ethical dilemma for physicians: Should$\backslash$nthe physician do as the couple requests, and there with respect the$\backslash$nautonomy of patients, or adhere to medical policy that takes the health$\backslash$nof the mother and children at heart This article provides an exploration$\backslash$nof the arguments found in the literature that plays a role in the$\backslash$ndiscussion on this topic and eventually argues that what a physician$\backslash$nshould do depends on the specificities of the context in which patients$\backslash$nand physicians are implicated. These contextual issues can be taken into$\backslash$naccount in a shared decision-making procedure, which allows reflections$\backslash$nand the responsibilities of both patients and physicians to be attended$\backslash$nin decision about assisted reproduction.},
author = {Ezugwu, EC and der Burg, SVan},
doi = {10.4103/2141-9248.149761},
issn = {2141-9248},
journal = {Annals of Medical and Health Sciences Research},
keywords = {autonomy,embryo transfer,in vitro fertilization,multiple pregnancy,values and culture},
number = {1},
pages = {1},
pmid = {25745568},
title = {Debating elective single embryo transfer after in vitro fertilization: A plea for a context-sensitive approach},
volume = {5},
year = {2015}
}
