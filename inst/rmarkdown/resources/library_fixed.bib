language = {en},
month = jan,
number = {1},
pages = {28--39},
shorttitle = {Pregnancy and Neonatal Outcomes of Aboriginal Wome},
title = {Pregnancy and Neonatal Outcomes of Aboriginal Women: A Systematic Review and Meta-Analysis},
volume = {21},
year = {2011}
