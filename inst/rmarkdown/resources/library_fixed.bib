@article{Antonarakis2012a,
abstract = {Objective: To describe metastasis-free survival (MFS) in men with prostate-specific antigen (PSA) recurrence following radical prostatectomy, and to define clinical prognostic factors modifying metastatic risk. Patients and Methods: We conducted a retrospective analysis of 450 men treated with prostatectomy at a tertiary hospital between July 1981 and July 2010 who developed PSA recurrence (â¥0.2 ng/mL) and never received adjuvant or salvage therapy before the development of metastatic disease. We estimated MFS using the Kaplan-Meier method, and investigated factors influencing the risk of metastasis using Cox proportional hazards regression. Results: Median follow-up after prostatectomy was 8.0 years, and after biochemical recurrence was 4.0 years. At last follow-up, 134 of 450 patients (29.8{\%}) had developed metastases, while median MFS was 10.0 years. Using multivariable regressions, two variables emerged as independently predictive of MFS: PSA doubling time ({\textless}3.0 vs 3.0-8.9 vs 9.0-14.9 vs â¥15.0 months) and Gleason score (â€6 vs 7 vs 8-10). Using these stratifications of Gleason score and PSA doubling time, tables were constructed to predict median, 5- and 10-year MFS after PSA recurrence. In different patient subsets, median MFS ranged from 1 to 15 years. Conclusions: In men undergoing prostatectomy, MFS after PSA recurrence is variable and is most strongly influenced by PSA doubling time and Gleason score. These parameters serve to stratify men into different risk groups with respect to metastatic progression. Our findings may provide the background for appropriate selection of patients, treatments and endpoints for clinical trials. {\textcopyright} 2011 The Authors.},
author = {Antonarakis, Emmanuel S. and Feng, Zhaoyong and Trock, Bruce J. and Humphreys, Elizabeth B. and Carducci, Michael A. and Partin, Alan W. and Walsh, Patrick C. and Eisenberger, Mario A.},
doi = {10.1111/j.1464-410X.2011.10422.x},
issn = {14644096},
journal = {BJU International},
keywords = {PSA recurrence,metastasis-free survival,natural history,prostate cancer},
number = {1},
pages = {32--39},
title = {The natural history of metastatic progression in men with prostate-specific antigen recurrence after radical prostatectomy: Long-term follow-up},
volume = {109},
year = {2012}
}
