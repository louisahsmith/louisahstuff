@article{Robins2000c,
abstract = {In both observational and randomized studies, subjects commonly drop out of the study (i.e., become censored) before end of follow-up. If, conditional on the history of the observed data up to t, the hazard of dropping out of the study (i.e., censoring) at time t does not depend on the possibly unobserved data subsequent to t, we say drop-out is ignorable or explainable (Rubin, 1976). On the other hand, if the hazard of drop-out depends on the possibly unobserved future, we say drop-out is non-ignorable or, equivalently, that there is selection bias on unobservables. Neither the existence of selection bias on unobservables nor its magnitude is identifiable from the joint distribution of the observables. In view of this fact, we argue that the data analyst should conduct a "sensitivity analysis" to quantify how one's inference concerning an outcome of interest varies as a function of the magnitude of non-identifiable selection bias.$\backslash$n},
address = {New York},
author = {Robins, James M. and Rotnitzky, Andrea and Scharfstein, Daniel O.},
doi = {10.1007/978-1-4612-1284-3_1},
editor = {Halloran, M. Elizabeth and Berry, Donald},
journal = {Statistical Models in Epidemiology, the Environment, and Clinical Trials},
pages = {1--94},
publisher = {Springer},
title = {Sensitivity Analysis for Selection bias and unmeasured Confounding in missing Data and Causal inference models},
year = {2000}
}
@inproceedings{Bareinboim2014,
abstract = {Selection bias is caused by preferential exclusion of units from the samples and represents a major obstacle to valid causal and statistical inferences; it cannot be removed by randomized experiments and can rarely be detected in either experimental or observational studies. In this paper, we provide complete graphical and algorithmic conditions for recovering conditional probabilities from selection biased data. We also provide graphical conditions for recoverability when unbiased data is available over a subset of the variables. Finally, we provide a graphical condition that generalizes the backdoor criterion and serves to recover causal effects when the data is collected under preferential selection.},
author = {Bareinboim, Elias and Tian, Jin and Pearl, Judea},
booktitle = {Proceedings of the Twenty-Eighth AAAI Conference on Artificial Intelligence},
isbn = {9781577356806},
keywords = {causal inference,causality,sampling bias,selection bias,statistical inference},
pages = {2410--2416},
title = {Recovering from Selection Bias in Causal and Statistical Inference},
year = {2014}
}
