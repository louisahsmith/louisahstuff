isbn = {1476-5497 (Electronic)},
issn = {1476-5497},
journal = {International journal of obesity (2005)},
keywords = {*Body Mass Index,*Obesity,*Pregnancy Complications,Adiposity,Adult,Attention Deficit Disorder with Hyperactivity,Attention Deficit Disorder with Hyperactivity/*epi,Attention Deficit Disorder with Hyperactivity: epi,Attention Deficit Disorder with Hyperactivity: eti,Body Mass Index,Child,Child, Preschool,Child- Preschool,Cohort Studies,Denmark,Denmark/epidemiology,Denmark: epidemiology,Female,Finland,Finland/epidemiology,Finland: epidemiology,Humans,Male,Mothers,Obesity,Pregnancy,Pregnancy Complications,Prospective Studies,Sweden,Sweden/epidemiology,Sweden: epidemiology},
