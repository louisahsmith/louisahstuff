issn = {1687-9589},
journal = {Obstetrics and Gynecology International},
language = {en},
pages = {1--9},
shorttitle = {Risk Factors for Preterm Birth among HIV-Infected },
title = {Risk Factors for Preterm Birth among HIV-Infected Tanzanian Women: A Prospective Study},
