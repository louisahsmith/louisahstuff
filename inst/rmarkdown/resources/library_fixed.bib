keywords = {Breast neoplasm etiology,Induced abortion,Spontaneous abortion},
number = {3},
pages = {443--448},
pmid = {15095312},
title = {Null association between pregnancy termination and breast cancer in a registry-based study of parous women},
volume = {110},
year = {2004}
}
