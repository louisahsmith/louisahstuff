@article{Hernan2018a,
abstract = {When using observational data, quantifying the effect of treatment duration on survival outcomes is not straightforward because only people who live for a long time can receive treatment for a long time. This problem doesn't apply to randomised trials because people are classified based on the treatment duration they are assigned, rather than the treatment duration that they achieve. This approach accepts that dead people do not deviate from their assigned treatment strategy. By transferring this insight to the analysis of observational data, we can follow three steps to estimate the effect of treatment duration from observational data without the bias of naive comparisons between long term and short term users. The first step is cloning people to assign them to multiple treatment strategies. The second step is censoring clones when they deviate from their assigned treatment strategy. The third step is performing inverse probability weighting to adjust for the potential selection bias introduced by censoring. The procedure can be used to compare any treatment strategies that are sustained over time. Cloning, censoring, and weighting eliminates immortal time bias in the estimates of absolute and relative risk, which helps researchers focus their attention on other biases that may be present in observational analyses and are not so easily eliminated. {\#}{\#}{\#} Summary box},
author = {Hern{\'{a}}n, Miguel A},
doi = {10.1136/bmj.k182},
issn = {0959-8138},
journal = {BMJ},
month = feb,
pages = {k182},
title = {How to estimate the effect of treatment duration on survival outcomes using observational data},
volume = {360},
year = {2018}
}
@article{Applebaum2007,
abstract = {OBJECTIVE: To explore the healthy worker survivor effect (HWSE) in a study of Vermont granite workers by distinguishing "prevalent" from "incident" hires based on date of hire before or after the start of follow-up. METHODS: Records of workers between 1950 and 1982 were obtained from a medical surveillance programme. Proportional hazards models were used to model the association between silica exposure and lung cancer mortality, with penalised splines used to smooth the exposure-response relationship. A sensitivity analysis compared results between the original cohort and subcohorts defined by restricting date of hire to include varying proportions of prevalent hires. RESULTS: Restricting to incident hires reduced the 213 cases by 74{\%} and decreased the exposure range. The maximum mortality rate ratio (MRR) was close to twofold in all subcohorts. However, the exposure at which the maximum MRR was achieved decreased from 4.0 to 0.6 mg-year/m3 as the proportion of prevalent hires decreased from 50{\%} in the original cohort to 0{\%} in the subcohort of incident hires. CONCLUSION: Despite loss in power and restricted exposure range, decreasing the relative proportion of prevalent to incident hires reduced HWSE bias, resulting in stronger evidence for a dose-response between silica exposure and lung cancer mortality.},
author = {Applebaum, Katie M. and Malloy, Elizabeth J. and Eisen, Ellen A.},
doi = {10.1136/oem.2006.031369},
isbn = {1470-7926 (Electronic)$\backslash$r1351-0711 (Linking)},
issn = {13510711},
journal = {Occupational and Environmental Medicine},
number = {10},
pages = {681--687},
pmid = {17449560},
title = {Reducing healthy worker survivor bias by restricting date of hire in a cohort study of Vermont granite workers},
volume = {64},
year = {2007}
}
